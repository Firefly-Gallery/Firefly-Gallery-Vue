{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "exclude": [
    "node_modules",
    "src/**/*.vue",
    "src/**/*.js"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "types": ["vue"],
    "allowJs": true,
    "paths": {
      "@/*": ["src/*"]
    }
  }
}